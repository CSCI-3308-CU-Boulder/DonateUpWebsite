{
"name": "first",
"version": "1.0",
"manifest_version": 2,
"content_scripts": [
    {
    "matches": [
      
      "http://*/cart",
      "https://*/cart",
      "https://*/us/cart"
      
    ],
    "js": ["content.js"]
    }
    
  ],
  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "/templates/popup.html",
    "default_title": "Yay"

  },
  
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "web_accessible_resources": [
   "templates/injected_popup.html"
  ]
}
